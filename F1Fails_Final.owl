@prefix : <http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix f1: <http://www.semanticweb.org/federicoprandi/ontologies/> .
@prefix bbc: <http://www.bbc.co.uk/ontologies/sport/#> .
@prefix dc1: <https://protege.stanford.edu/plugins/owl/dc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix veh: <http://ontology.eil.utoronto.ca/icity/Vehicle/#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix F1Fails: <http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/federicoprandi/ontologies/F1Fails> .

<http://www.semanticweb.org/federicoprandi/ontologies/F1Fails> rdf:type owl:Ontology ;
                                                                owl:imports <http://www.ontologydesignpatterns.org/cp/owl/bag.owl> ;
                                                                dc:author "Federico Prandi"@en ;
                                                                dc:date "5/07/2023"@en ;
                                                                dc:description "This ontology want to describe the Accidents happened on the Formula 1 Races."@en ;
                                                                dc:title "Formula 1 Fails Ontology"@en ;
                                                                rdfs:label "F1Fails"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/author
dc:author rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  https://protege.stanford.edu/plugins/owl/dc/dublincore.owlauthor
dc1:dublincore.owlauthor rdf:type owl:AnnotationProperty .


###  https://protege.stanford.edu/plugins/owl/dc/dublincore.owldate
dc1:dublincore.owldate rdf:type owl:AnnotationProperty .


###  https://protege.stanford.edu/plugins/owl/dc/dublincore.owldescription
dc1:dublincore.owldescription rdf:type owl:AnnotationProperty .


###  https://protege.stanford.edu/plugins/owl/dc/dublincore.owltitle
dc1:dublincore.owltitle rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Car_Involved_In
F1Fails:Car_Involved_In rdf:type owl:ObjectProperty ;
                        owl:inverseOf F1Fails:Event_involved_Vehicle ;
                        rdfs:domain F1Fails:Vehicle ;
                        rdfs:range F1Fails:Event ;
                        rdfs:comment "A car that is involved in accident of some type"@en ;
                        rdfs:label "Car Involved in"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driven_By
F1Fails:Driven_By rdf:type owl:ObjectProperty ;
                  owl:inverseOf F1Fails:Drives ;
                  rdfs:comment "Car driven by some driver"@en ;
                  rdfs:label "Driven by"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver_Involved_In
F1Fails:Driver_Involved_In rdf:type owl:ObjectProperty ;
                           owl:inverseOf F1Fails:Event_involved_the_driver ;
                           rdf:type owl:TransitiveProperty ;
                           rdfs:domain F1Fails:Driver_F1 ;
                           rdfs:range F1Fails:Event ;
                           owl:propertyChainAxiom ( F1Fails:Drives
                                                    F1Fails:Car_Involved_In
                                                  ) ;
                           rdfs:comment """Returns Driver involved in at least one event. It uses a chain property:
Driver Drives a Car -> a Car is involved in an event -> so the driver is involved the event"""@en ;
                           rdfs:label "Driver involved in"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver_Work_For
F1Fails:Driver_Work_For rdf:type owl:ObjectProperty ;
                        owl:inverseOf F1Fails:Team_Driver_is ;
                        rdf:type owl:TransitiveProperty ;
                        rdfs:domain F1Fails:Driver ;
                        rdfs:range F1Fails:Team ;
                        owl:propertyChainAxiom ( F1Fails:Drives
                                                 F1Fails:Owned_By
                                               ) ;
                        rdfs:comment "The driver works for a specific team"@en ;
                        rdfs:label "Driver Works For"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Drives
F1Fails:Drives rdf:type owl:ObjectProperty ;
               rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
               rdfs:range F1Fails:Vehicle ;
               rdfs:comment "A driver drives a car"@en ;
               rdfs:label "Drives"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event_Happened_In_Season
F1Fails:Event_Happened_In_Season rdf:type owl:ObjectProperty ;
                                 owl:inverseOf F1Fails:Season_With_Event ;
                                 rdfs:domain F1Fails:Event ;
                                 rdfs:range F1Fails:Seasons ;
                                 rdfs:comment "An Event happen into a specific season"@en ;
                                 rdfs:label "Event Happened in Season"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event_Happened_in_Track
F1Fails:Event_Happened_in_Track rdf:type owl:ObjectProperty ;
                                owl:inverseOf F1Fails:Track_Involved_In_Event ;
                                rdf:type owl:FunctionalProperty ;
                                rdfs:domain F1Fails:Event ;
                                rdfs:range F1Fails:Track ;
                                rdfs:comment "An Event happed into a specific track"@en ;
                                rdfs:label "Event Happened in a Track"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event_Involve_The_Team
F1Fails:Event_Involve_The_Team rdf:type owl:ObjectProperty ;
                               owl:inverseOf F1Fails:Team_Involved_In_Event ;
                               rdf:type owl:TransitiveProperty ;
                               rdfs:domain F1Fails:Event ;
                               rdfs:range F1Fails:Team ;
                               owl:propertyChainAxiom ( F1Fails:Event_involved_Vehicle
                                                        F1Fails:Owned_By
                                                      ) ;
                               rdfs:comment """A team is inolved into a specific Event
An Event Crashed with some Vehicle -> Vheicle is owned by a Team -> Event involve a Team"""@en ;
                               rdfs:label "Event involved the Team"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event_involved_Vehicle
F1Fails:Event_involved_Vehicle rdf:type owl:ObjectProperty ;
                               rdfs:domain F1Fails:Event ;
                               rdfs:range F1Fails:Vehicle ;
                               rdfs:comment "An Event crash with some vehicle"@en ;
                               rdfs:label "Event involved the Vehicle"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event_involved_the_driver
F1Fails:Event_involved_the_driver rdf:type owl:ObjectProperty ,
                                           owl:TransitiveProperty ;
                                  rdfs:domain F1Fails:Event ;
                                  rdfs:range F1Fails:Driver_F1 ;
                                  rdfs:comment "An event involved a driver into that"@en ;
                                  rdfs:label "Event involved the Driver"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Owned_By
F1Fails:Owned_By rdf:type owl:ObjectProperty ;
                 owl:inverseOf F1Fails:Owns ;
                 rdfs:domain F1Fails:Vehicle ;
                 rdfs:range F1Fails:Team ;
                 rdfs:comment "A car is owned by a team"@en ;
                 rdfs:label "Owned By"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Owns
F1Fails:Owns rdf:type owl:ObjectProperty ;
             rdfs:domain F1Fails:Team ;
             rdfs:range F1Fails:Vehicle ;
             rdfs:comment "A team owns a car"@en ;
             rdfs:label "Owns"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Season_With_Event
F1Fails:Season_With_Event rdf:type owl:ObjectProperty ;
                          rdfs:domain F1Fails:Seasons ;
                          rdfs:range F1Fails:Event ;
                          rdfs:comment "In a season happen an event"@en ;
                          rdfs:label "Season with Event"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Seasons_has_the_Team
F1Fails:Seasons_has_the_Team rdf:type owl:ObjectProperty ;
                             owl:inverseOf F1Fails:Team_Run_In_Season ;
                             rdfs:domain F1Fails:Seasons ;
                             rdfs:range F1Fails:Team ;
                             rdfs:comment "Contains all the teams that run into that season"@en ;
                             rdfs:label "Season has the Teams"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Team_Driver_is
F1Fails:Team_Driver_is rdf:type owl:ObjectProperty ;
                       rdfs:domain F1Fails:Team ;
                       rdfs:range F1Fails:Driver ;
                       rdfs:comment "People who work for a team"@en ;
                       rdfs:label "Team Driver is"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Team_Involved_In_Event
F1Fails:Team_Involved_In_Event rdf:type owl:ObjectProperty ,
                                        owl:TransitiveProperty ;
                               rdfs:domain F1Fails:Team ;
                               rdfs:range F1Fails:Event ;
                               rdfs:comment "A Team involved in an Event"@en ;
                               rdfs:label "Team Involved In Event"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Team_Run_In_Season
F1Fails:Team_Run_In_Season rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain F1Fails:Team ;
                           rdfs:range F1Fails:Seasons ;
                           rdfs:comment "A team run into a season"@en ;
                           rdfs:label "Team run in Season"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Track_Involved_In_Event
F1Fails:Track_Involved_In_Event rdf:type owl:ObjectProperty ;
                                rdfs:domain F1Fails:Track ;
                                rdfs:range F1Fails:Event ;
                                rdfs:comment "A track used into a season like a grandPrix"@en ;
                                rdfs:label "Track involved in Event"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Car
F1Fails:Car rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#DriverName
F1Fails:DriverName rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf F1Fails:Driver_F1 ;
                   rdfs:comment "Name of the driver"@en ;
                   rdfs:label "Driver Name"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver_F1
F1Fails:Driver_F1 rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event
F1Fails:Event rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#EventType
F1Fails:EventType rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf F1Fails:Event ;
                  rdfs:domain F1Fails:Event ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "Crash" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Failure" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "HumanError" ;
                                                                 rdf:rest rdf:nil
                                                               ]
                                                    ]
                                         ]
                             ] ;
                  rdfs:comment "Type of the event"@en ;
                  rdfs:label "Event Type"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Season
F1Fails:Season rdf:type owl:DatatypeProperty ;
               rdfs:domain F1Fails:Seasons ;
               rdfs:comment "A F1 Season"@en ;
               rdfs:label "Season"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#SeasonYear
F1Fails:SeasonYear rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf F1Fails:Season ;
                   rdfs:domain F1Fails:Seasons ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "A year of the Season"@en ;
                   rdfs:label "Season Year"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Team
F1Fails:Team rdf:type owl:DatatypeProperty ;
             rdfs:domain F1Fails:Team .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TeamName
F1Fails:TeamName rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf F1Fails:Team ;
                 rdfs:comment "Name of the team"@en ;
                 rdfs:label "Team Name"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TeamSeason
F1Fails:TeamSeason rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf F1Fails:Team ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "Season in which the team raced"@en ;
                   rdfs:label "Team Season"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Track
F1Fails:Track rdf:type owl:DatatypeProperty ;
              rdfs:domain F1Fails:Track .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TrackLength
F1Fails:TrackLength rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf F1Fails:Track ;
                    rdfs:range xsd:integer ;
                    rdfs:comment "Track length in meter"@en ;
                    rdfs:label "Track Length"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TrackLocation
F1Fails:TrackLocation rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf F1Fails:Track ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Track name"@en ;
                      rdfs:label "Track Location"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TrackNation
F1Fails:TrackNation rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf F1Fails:Track ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Nation of the track"@en ;
                    rdfs:label "Track Nation"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TrackType
F1Fails:TrackType rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf F1Fails:Track ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "Race" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Street" ;
                                                      rdf:rest rdf:nil
                                                    ]
                                         ]
                             ] ;
                  rdfs:comment "Type of the track enumerated for Race And Street"@en ;
                  rdfs:label "Track Type"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#TrackUsed
F1Fails:TrackUsed rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf F1Fails:Track ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "Set if it is still used"@en ;
                  rdfs:label "Track Used"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#WheelsNumber
F1Fails:WheelsNumber rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf F1Fails:Car ;
                     rdfs:range xsd:integer ;
                     rdfs:comment "Number of wheels"@en ;
                     rdfs:label "Wheels Number"@en .


#################################################################
#    Classes
#################################################################

###  http://ontology.eil.utoronto.ca/icity/Vehicle/#Vehicle
veh:Vehicle rdf:type owl:Class ;
            owl:equivalentClass F1Fails:Vehicle ;
            rdfs:comment "External ontology about vehicle" ;
            rdfs:label "iCity - Vehicle" ;
            rdfs:seeAlso "https://enterpriseintegrationlab.github.io/icity/Vehicle/doc/index-en.html#Vehicle" .


###  http://www.bbc.co.uk/ontologies/sport/#SportingOrganisation
bbc:SportingOrganisation rdf:type owl:Class ;
                         owl:equivalentClass F1Fails:Team ;
                         rdfs:comment "External onology about teams" ;
                         rdfs:label "BBC - Team" ;
                         rdfs:seeAlso "https://iptc.org/thirdparty/bbc-ontologies/sport.html" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Alfa_Romeo
F1Fails:Alfa_Romeo rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty F1Fails:TeamName ;
                                         owl:hasValue "Alfa Romeo"
                                       ] ;
                   rdfs:subClassOf F1Fails:Team ;
                   rdfs:comment "Teams leads by RedBull"@en ;
                   rdfs:label "Alfa Romeo"@it .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#AstonMartin
F1Fails:AstonMartin rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty F1Fails:TeamName ;
                                          owl:hasValue "Aston Martin"
                                        ] ;
                    rdfs:subClassOf F1Fails:Team ;
                    owl:disjointWith F1Fails:Team_Ferrari ;
                    rdfs:comment "Teams leads by Aston MArtin" ;
                    rdfs:label "Aston Martin" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Car
F1Fails:Car rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty F1Fails:WheelsNumber ;
                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:onDatatype xsd:integer ;
                                                       owl:withRestrictions ( [ xsd:minExclusive 3
                                                                              ]
                                                                            )
                                                     ]
                                ] ;
            rdfs:subClassOf F1Fails:Vehicle ;
            owl:disjointWith F1Fails:Moto .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Car_F1
F1Fails:Car_F1 rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty F1Fails:Driven_By ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass F1Fails:Driver_F1
                                   ] ;
               rdfs:subClassOf F1Fails:Car ;
               rdfs:comment "A car that compete into a Formula 1 Series" ;
               rdfs:label "Car F1" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Car_F1_Crashed
F1Fails:Car_F1_Crashed rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty F1Fails:Car_Involved_In ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass F1Fails:Event
                                           ] ;
                       rdfs:subClassOf F1Fails:Car_F1 ;
                       rdfs:comment "Car involved into a car crash" ;
                       rdfs:label "Car F1 Crashed" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Crash
F1Fails:Crash rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty F1Fails:EventType ;
                                    owl:hasValue "Crash"
                                  ] ;
              rdfs:subClassOf F1Fails:Event ;
              owl:disjointWith F1Fails:Failure ,
                               F1Fails:Human_Error ;
              rdfs:comment "Car crash event between two vehicle" ;
              rdfs:label "Crash" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver
F1Fails:Driver rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty F1Fails:Drives ;
                                     owl:someValuesFrom F1Fails:Vehicle
                                   ] ;
               rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
               rdfs:comment "A person who drive some vehicles"@en ;
               rdfs:label "Driver"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver_F1
F1Fails:Driver_F1 rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty F1Fails:Driver_Work_For ;
                                        owl:someValuesFrom F1Fails:Team
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty F1Fails:Drives ;
                                        owl:someValuesFrom F1Fails:Car_F1
                                      ] ;
                  rdfs:subClassOf F1Fails:Driver .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Driver_F1_Involved_In_Event
F1Fails:Driver_F1_Involved_In_Event rdf:type owl:Class ;
                                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                                          owl:onProperty F1Fails:Driver_Involved_In ;
                                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass F1Fails:Event
                                                        ] ;
                                    rdfs:subClassOf F1Fails:Driver_F1 ;
                                    rdfs:comment "A Dirver involved into an Event"@en ;
                                    rdfs:label "Driver F1 with Event"@en .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#European
F1Fails:European rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty F1Fails:TrackNation ;
                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                            owl:oneOf [ rdf:type rdf:List ;
                                                                        rdf:first "Italy" ;
                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                   rdf:first "Monaco" ;
                                                                                   rdf:rest rdf:nil
                                                                                 ]
                                                                      ]
                                                          ]
                                     ] ;
                 rdfs:subClassOf F1Fails:Track ;
                 owl:disjointWith F1Fails:Extra_European ;
                 rdfs:comment "A track placed into the Europe" ;
                 rdfs:label "European" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Event
F1Fails:Event rdf:type owl:Class .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Extra_European
F1Fails:Extra_European rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty F1Fails:TrackNation ;
                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                  owl:datatypeComplementOf [ rdf:type rdfs:Datatype ;
                                                                                             owl:oneOf [ rdf:type rdf:List ;
                                                                                                         rdf:first "Italy" ;
                                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                                    rdf:first "Monaco" ;
                                                                                                                    rdf:rest rdf:nil
                                                                                                                  ]
                                                                                                       ]
                                                                                           ]
                                                                ]
                                           ] ;
                       rdfs:subClassOf F1Fails:Track ;
                       rdfs:comment "A track placed extra europe" ;
                       rdfs:label "Extra - European" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Failure
F1Fails:Failure rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty F1Fails:EventType ;
                                      owl:hasValue "Failure"
                                    ] ;
                rdfs:subClassOf F1Fails:Event ;
                owl:disjointWith F1Fails:Human_Error ;
                rdfs:comment "Car Feilure Event on a vehicle" ;
                rdfs:label "Failure" .


###  http://www.semanticweb.org/federicoprandi/ontologies/F1Fails#Human_Error
F1Fails:Human_Error rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty F1Fails:EventType ;
                                          owl:hasValue "HumanError"
